{
  "name": "my-js-lib",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "run": "ncc run main.ts < input.txt",
    "build": "ncc build main.ts -o ./",
    "lint": "eslint ./",
    "lint-fix": "eslint --fix ./"
  },
  "prettier": {
    "semi": false
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tyahha/my-js-lib.git"
  },
  "author": "Toshinori Sato",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tyahha/my-js-lib/issues"
  },
  "homepage": "https://github.com/tyahha/my-js-lib#readme",
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "12",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
